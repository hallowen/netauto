enable
!
conf t
hostname CSR7
!
line console 0
logging sync
exec-timeout 0
!
username ansible priv 15 secret ansible
!
line vty 0 4
transport input ssh
login local
exit
!
ip domain name snyderhalos.lab
!
no ip domain lookup
do write
!
exit
!
crypto key generate rsa modulus 2048
!
write
!
conf t
!
ip ssh version 2
!
vrf definition VRF-MGMT
rd 15:15
address-family ipv4 unicast
exit
!
interface G2
vrf forwarding VRF-MGMT
ip address 192.168.0.37 255.255.255.0
no shut
exit
!
do write
